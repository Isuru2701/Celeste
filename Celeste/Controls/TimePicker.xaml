<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Celeste.Controls"
             xmlns:hc="https://handyorg.github.io/handycontrol" x:Class="Celeste.Controls.TimePicker"
             mc:Ignorable="d" 
             d:DesignHeight="800" d:DesignWidth="1000">

    <UserControl.Background>
        <SolidColorBrush Color="Black" Opacity="0.5"/>
    </UserControl.Background>
    
    <Grid>



        <Button x:Name="btn_back"
                Style="{StaticResource DiamondButtonTheme}" 
                Margin="61,365,869,365"
                Click="btn_back_Click">
            <Image Source="../Resources/back_btn.png"/>
        </Button>


    </Grid>
</UserControl>
